<% include ../partials/header %>
<div class = 'container'>
<h1>Edit Your restaurants</h1>
<form action = '/restaurants/<%= foundrest._id %>?_method=PUT' method = 'POST'>
    <div class='form-group'>
    <label for='InputName'>Yelprest Name</label>
    <input type='Name' class='form-control' name = 'restaurant[name]' id='InputName' value =<%= foundrest.name %>>
  </div>
  <div class='form-group'>
    <label for='InputUrl'>Url</label>
    <input type='Url' class='form-control' name = 'restaurant[img]' id='InputUrl' value = <%= foundrest.img %>>
  </div>
   <div class='form-group'>
    <label for='InputDes'>Description</label>
    <input type='text' class='form-control' name = 'restaurant[description]' id='InputDes' value = <%= foundrest.description %> >
  </div>
  <button type='submit' class='btn btn-primary'>Submit</button>
</form>
</div>

<% include ../partials/footer %>